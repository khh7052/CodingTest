#include <iostream>
using namespace std;
int main() {
	int n1, n2;
	int d;
	cin >> n1 >> n2 >> d; 

	for(int i = 0; i < d; i++){
		if(i % 2 == 0){
			int toss = (n1 + 1) / 2;
			n1 -= toss;
			n2 += toss;
		}
		else{
			int toss = (n2 + 1) / 2;
			n2 -= toss;
			n1 += toss;
		}
	}
	
	cout << n1 << " " << n2 << endl;
	return 0;
}